<view class="container"><view class="item"><text class="title">血压</text><input type="text" placeholder="90-140" data-event-opts="{{[['input',[['__set_model',['','BP','$event',[]]]]]]}}" value="{{BP}}" bindinput="__e"/><text>mmHg</text></view><view class="item"><text class="title">心率</text><input type="text" placeholder="60-100" data-event-opts="{{[['input',[['__set_model',['','BPM','$event',[]]]]]]}}" value="{{BPM}}" bindinput="__e"/><text>次/分钟</text></view><view class="item"><text class="title">空腹血糖</text><input type="text" placeholder="3.9-6.1" data-event-opts="{{[['input',[['__set_model',['','GLU','$event',[]]]]]]}}" value="{{GLU}}" bindinput="__e"/><text>mol</text></view><view class="item"><text class="title">总胆固醇</text><input type="text" placeholder="3.1-5.7" data-event-opts="{{[['input',[['__set_model',['','TCHO','$event',[]]]]]]}}" value="{{TCHO}}" bindinput="__e"/><text>mmol/l</text></view><view class="item"><text class="title">甘油三酯</text><input type="text" placeholder="0.4-1.7" data-event-opts="{{[['input',[['__set_model',['','TG','$event',[]]]]]]}}" value="{{TG}}" bindinput="__e"/><text>mmol/l</text></view><view class="item"><text class="title ">尿酸</text><input type="text" placeholder="89-360" data-event-opts="{{[['input',[['__set_model',['','acid','$event',[]]]]]]}}" value="{{acid}}" bindinput="__e"/><text>μmol/L</text></view><view class="item itemo"><text class="title">低密度脂蛋白</text><input type="text" placeholder="1-3.4" data-event-opts="{{[['input',[['__set_model',['','LDL','$event',[]]]]]]}}" value="{{LDL}}" bindinput="__e"/><text>mmol/L</text></view><view class="item itemo"><text class="title ">记录时间</text><input type="text" data-event-opts="{{[['input',[['__set_model',['','record_time','$event',[]]]]]]}}" value="{{record_time}}" bindinput="__e"/><text></text></view><view class="item itemo"><text class="title ">情况说明</text><input type="text" data-event-opts="{{[['input',[['__set_model',['','condition','$event',[]]]]]]}}" value="{{condition}}" bindinput="__e"/><text></text></view><button type="primary" data-event-opts="{{[['tap',[['submit']]]]}}" bindtap="__e">提交</button><u-toast class="vue-ref" vue-id="1" data-ref="uToast" bind:__l="__l"></u-toast></view>