<view class="content"><view class="header"><image src="../../static/shilu-login/logo.png"></image></view><view class="list"><view class="list-call"><image class="img" src="/static/shilu-login/1.png"></image><input class="sl-input" type="number" maxlength="11" placeholder="手机号" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/></view><view class="list-call"><image class="img" src="/static/shilu-login/2.png"></image><input class="sl-input" type="text" maxlength="32" placeholder="登录密码" password="{{!showPassword}}" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/><image class="img" src="{{showPassword?'/static/shilu-login/op.png':'/static/shilu-login/cl.png'}}" data-event-opts="{{[['tap',[['display',['$event']]]]]}}" bindtap="__e"></image></view><view class="list-call"><image class="img" src="/static/shilu-login/2.png"></image><input class="sl-input" type="text" maxlength="32" placeholder="确认登录密码" password="{{!reshowPassword}}" data-event-opts="{{[['input',[['__set_model',['','repassword','$event',[]]]]]]}}" value="{{repassword}}" bindinput="__e"/><image class="img" src="{{reshowPassword?'/static/shilu-login/op.png':'/static/shilu-login/cl.png'}}" data-event-opts="{{[['tap',[['redisplay',['$event']]]]]}}" bindtap="__e"></image></view><view class="list-call"><image class="img" src="/static/shilu-login/3.png"></image><input class="sl-input" type="text" placeholder="邮箱" data-event-opts="{{[['input',[['__set_model',['','email','$event',[]]]]]]}}" value="{{email}}" bindinput="__e"/></view></view><view class="button-login" hover-class="button-hover" data-event-opts="{{[['tap',[['bindLogin',['$event']]]]]}}" bindtap="__e"><text>注册</text></view><view class="agreement"><image src="{{agreement==true?'/static/shilu-login/ty1.png':'/static/shilu-login/ty0.png'}}" data-event-opts="{{[['tap',[['agreementSuccess',['$event']]]]]}}" bindtap="__e"></image><text data-event-opts="{{[['tap',[['agreementSuccess',['$event']]]]]}}" bindtap="__e">同意</text><navigator url="agreement?id=1" open-type="navigate">《软件用户协议》</navigator></view></view>